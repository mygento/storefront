{
  "private": true,
  "name": "default-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx,graphql}' && stylelint 'src/**/*.{css,scss}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx,graphql}' --fix && stylelint 'src/**/*.{css,scss}' --fix",
