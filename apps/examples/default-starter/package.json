    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "babel-loader": "^8.2.5",
    "dotenv": "^16.0.1",
    "eslint": "8.20.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "next-plugin-graphql": "^0.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.49.0",
    "serve": "13.0.2",
    "storybook-addon-next": "1.3.1",
